		MDY(2,3,2015) LE SRVC_DT LE MDY(12,31,2017) THEN NOVEL_THERAPY = "YES" ;

	/*ibrutinib (IMBRUVICA) - PART D*/
	IF NDC9 in ('579620140') and
		CTYPE IN ("Lymphoma") AND
		MDY(1,29,2015) LE SRVC_DT LE MDY(12,31,2017) THEN NOVEL_THERAPY = "YES" ;


